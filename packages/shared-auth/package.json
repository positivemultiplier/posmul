      "types": "./dist/use-cases/index.d.ts",
      "default": "./dist/use-cases/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json --skipLibCheck --noEmitOnError false",
    "dev": "tsc --watch",
