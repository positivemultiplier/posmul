{
  "name": "@posmul/shared-auth",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./repository": {
      "types": "./dist/repository/index.d.ts",
      "default": "./dist/repository/index.js"
    },
    "./services": {
      "types": "./dist/services/index.d.ts", 
      "default": "./dist/services/index.js"
    },
