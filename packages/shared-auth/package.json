    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@posmul/shared-types": "workspace:*",
    "@supabase/supabase-js": "^2.43.2"
  },
  "peerDependencies": {
    "next": ">=13.0.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@types/node": "^20.0.0",
    "next": "^15.3.4"
  }
